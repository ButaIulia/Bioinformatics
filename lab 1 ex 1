#include <iostream>
#include <set>
#include <string>
using namespace std;

int main() {
    string sequence;
    cout << "Enter sequence: ";
    getline(cin, sequence);

    set<char> alphabet;  
    for (char c : sequence) {
        if (c != ' ' && c != ',')  
            alphabet.insert(c);
    }
    cout << "Alphabet: { ";
    for (auto &sym : alphabet) {
        cout << sym << " ";
    }
    cout << "}" << endl;

    return 0;
}
